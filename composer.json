{
  "name": "petar/repeat-toolkit",
  "description": "Repeat Toolkit for Laravel: AbstractModel, controllers, macros, helpers and utilities.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Petar Ristovic",
      "email": "you@example.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/database": "^10.0|^11.0",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/translation": "^10.0|^11.0",
    "illuminate/auth": "^10.0|^11.0",
    "illuminate/http": "^10.0|^11.0",
    "illuminate/routing": "^10.0|^11.0",
    "illuminate/view": "^10.0|^11.0",
    "symfony/process": "^6.0|^7.0",
    "gettext/gettext": "^5.7",
    "tightenco/ziggy": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "RepeatToolkit\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "RepeatToolkit\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RepeatToolkit\\Providers\\RouteMacroServiceProvider",
        "RepeatToolkit\\Providers\\ToolkitServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-main": "0.1-dev"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "npm install ziggy-js --no-audit --no-fund || true"
    ],
    "post-update-cmd": [
      "npm install ziggy-js --no-audit --no-fund || true"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
